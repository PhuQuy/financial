{
  "hosting": {
    "public": "functions/dist/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
            "source": "/seo/**",
            "function": "http"
          },
          {
            "source": "**",
            "destination": "/index.html"
          }
      ]
  }
}
