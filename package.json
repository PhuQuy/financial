{
  "name": "angular6",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server:prod": "ng run angular6:server && webpack --config webpack.server.config.js --progress --colors",
    "build:browser:prod": "ng build --prod --build-optimizer",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod && cp-cli dist/ functions/dist/ && cd functions && npm install",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:browser:serverless": "ng build --prod --base-href /",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "cp-cli dist/ functions/dist/ && cd functions && npm install && firebase deploy",
    "build:server:serverless": "ng run angular6:server && webpack --config webpack.server.config.js --progress --colors",
    "build:docker": "docker build -t docker-image-name ."
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/platform-server": "^6.0.0",
    "@angular/pwa": "^0.11.4",
    "@angular/router": "^6.1.10",
    "@angular/service-worker": "^6.1.10",
    "@ng-bootstrap/ng-bootstrap": "4.0.0",
    "@ng-toolkit/serverless": "^1.1.46",
    "@ng-toolkit/universal": "^1.1.45",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "angular-froala-wysiwyg": "^2.9.1",
    "angular2-froala-wysiwyg": "^2.6.0",
    "angularfire2": "5.0.0-rc.9",
    "bootstrap": "4.0.0",
    "bootstrap-select": "^1.13.5",
    "chart.js": "^2.7.3",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "cp-cli": "^1.1.0",
    "domino": "^2.1.0",
    "firebase": "5.0.4",
    "jsencrypt": "^3.0.0-rc.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "ts-loader": "4.2.0",
    "webpack-cli": "^2.1.4",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.1.10",
    "@types/node": "~8.9.4",
    "opencollective": "^1.0.3",
    "protractor": "~5.3.0",
    "ts-loader": "4.2.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-cli": "2.1.2"
  }
}
